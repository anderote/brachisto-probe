  "cross_effects": {
    "propulsion": {
      "description": "Lighter probes have more delta-v for same fuel",
      "formula": "effective_mass_ratio = base_ratio / materials_factor",
      "effect": "20% mass reduction = ~22% more delta-v"
    },
    "structures": {
      "description": "Lighter structures easier to build and transport",
      "formula": "build_cost = base_cost × materials_factor"
    },
    "mass_driver": {
      "description": "Stronger materials allow higher projectile acceleration",
      "formula": "max_g = base_g × strength_factor"
    },
    "space_elevator": {
      "description": "Requires specific strength > 50 MN·m/kg for Earth",
      "threshold_knm_kg": 50000,
      "earth_viable_tier": 6
    },
    "radiation": {
      "description": "Some materials have intrinsic radiation resistance",
