{
  "tree_id": "materials",
  "name": "Materials Science",
  "category": "dexterity",
  "description": "Structural materials affecting mass, strength, and durability. Lighter materials mean more delta-v and easier construction.",

  "physics_model": {
    "primary_effect": "mass_reduction_factor",
    "secondary_effects": ["tensile_strength", "radiation_resistance"],
    "key_metric": "specific_strength",
    "formula": "specific_strength = tensile_strength / density (N·m/kg)"
  },

  "display_format": {
    "primary": "{tensile_strength_gpa} GPa",
    "secondary": "({density_kg_m3} kg/m³)",
    "effect": "[{mass_factor}× mass, {strength_factor}× strength]",
    "example": "130 GPa (1000 kg/m³) [0.2× mass, 260× strength]"
  },

