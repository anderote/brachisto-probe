{
  "tree_id": "autonomy",
  "name": "Autonomy & Coordination",
  "category": "intelligence",
  "description": "Probe coordination and swarm intelligence. Reduces efficiency penalties when operating large numbers of probes.",

  "physics_model": {
    "primary_effect": "crowding_penalty_reduction",
    "formula": "efficiency = (1 - penalty)^log2(probe_count)",
    "example": "At 10¹² probes with 0.1% penalty: efficiency = 0.999^40 ≈ 96%"
  },

  "display_format": {
    "primary": "{crowding_penalty_percent}% penalty/doubling",
    "secondary": "({latency_tolerance})",
    "effect": "[{efficiency_at_1e12}% efficiency at 10¹² probes]",
    "example": "0.1% penalty/doubling (months latency) [96% efficiency at 10¹² probes]"
  },

