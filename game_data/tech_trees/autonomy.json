  "cross_effects": {
    "probe_efficiency": {
      "description": "Main effect - reduces crowding penalty at scale",
      "formula": "efficiency = (1 - penalty/100)^log2(probe_count)"
    },
    "research_speed": {
      "description": "Better coordination improves distributed research",
      "formula": "research_bonus = 1 + 0.05 × tier"
    },
    "build_rate": {
      "description": "Coordinated construction is faster",
      "formula": "build_bonus = 1 + 0.03 × tier"
    },
    "mining_efficiency": {
      "description": "Coordinated mining reduces overlap and waste",
      "formula": "mining_bonus = 1 + 0.02 × tier"
    },
    "transfer_efficiency": {
      "description": "Coordinated transfers optimize trajectories",
      "formula": "transfer_bonus = 1 + 0.01 × tier"
    }
  },

